    agent any  
    stages {  
            stage ('Build') {  
                echo "Building project done.." 
            }  
            stage ('Test') {  
                echo "Testing project done.." 
            }  
            stage ('Deploy') {  
                echo "Deploying project done.." 
            }  
            stage ('Monitor') {  
                echo "Monitoring project done.."  
            }  
    }  
}  
