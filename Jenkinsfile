pipeline {  
    agent any  
    stages {  
            stage ('Build') {  
                steps{
                     echo "Building Hello World program.." 
                }
            }  
            stage ('Test') { 
                 steps{
                    echo "Testing project done.." 
                 }
            }  
            stage ('Deploy') {  
                 steps{
                     echo "Deploying project done.." 
                 }
            }  
            stage ('Monitor') {  
                 steps{
                    echo "Monitoring project done.." 
                 }
            }  
    }  
}  
